<div class="mb-5 pb-4">

    <div class="row booking-details mt-3">
        <div class="col-1 p-0 m-auto">
            <div class="no-booking-circle"></div>
        </div>
        <div class="col pl-0">
           <!--  7:35 - 8:30 -->

        </div>
        <div class="col text-right time-remaining">
            55 minutes
        </div>
    </div>
<% @bookings.each do |booking| %>
    <div class="row booking-details mt-3">
        <div class="col-1 p-0">
            <div class="booked-shape status-in-queue"></div>
        </div>
        <div class="col">
            <div class="row">
                <%= booking.start.strftime("%H:%M") %>
            </div>
            <div class="col-1">
            </div>
            <div class="col booking-content">
                <div class="row pb-2">
                    <div class="col-2 my-auto">
                        <i class="fas fa-user"></i>
                    </div> 
                    <div class="col pl-3">
                        <%= booking.user.full_name %>
                    </div>
                </div>
                <div class="row pb-3">
                    <div class="col-2 my-auto">
                        <i class="fas fa-align-justify"></i>    
                    </div>
                    <div class="col pl-3">
                        <%= booking.description %>
                    </div>
                </div>
            </div>

            <div class="row">
                <%= booking.end.strftime("%H:%M") %>
            </div>
        </div>
        <div class="col-1 mr-5 mt-5 booking-content">
            <i class="fas fa-redo"></i>
        </div>
    </div>
<% end %>
